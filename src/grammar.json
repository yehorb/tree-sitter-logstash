{
  "name": "logstash",
  "rules": {
    "config": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "plugin_section"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "plugin_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "plugin_type"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "plugin_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "input"
        },
        {
          "type": "STRING",
          "value": "filter"
        },
        {
          "type": "STRING",
          "value": "output"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

