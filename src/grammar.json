{
  "name": "logstash",
  "rules": {
    "config": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "plugin_section"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "plugin_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "plugin_type"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "plugin"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "plugin_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "input"
        },
        {
          "type": "STRING",
          "value": "filter"
        },
        {
          "type": "STRING",
          "value": "output"
        }
      ]
    },
    "plugin": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "name": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_name_start"
        },
        {
          "type": "PATTERN",
          "value": "[A-Za-z0-9_-]+"
        }
      ]
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "STRING",
          "value": "=>"
        },
        {
          "type": "SYMBOL",
          "name": "_value"
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "plugin"
        },
        {
          "type": "SYMBOL",
          "name": "bareword"
        }
      ]
    },
    "bareword": {
      "type": "PATTERN",
      "value": "[A-Za-z_][A-Za-z0-9_]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_name_start"
    }
  ],
  "inline": [],
  "supertypes": []
}

